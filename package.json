    "license": "AGPL-3.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node -r tsconfig-paths/register src/app.ts\"",
        "build": "tsc",
        "start": "yarn build && cross-env NODE_ENV=production node -r module-alias/register dist/app.js",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
        "prettify": "eslint --fix --ext .ts src"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "connect-mongo": "^3.2.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "fp-ts": "^2.9.5",
        "mongodb": "^3.6.4",
        "node-ipc": "^9.1.3",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.9",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/mongodb": "^3.6.5",
        "@types/mongoose": "^5.10.3",
        "@types/node": "^14.14.25",
        "@types/node-ipc": "^9.1.3",
        "@types/passport": "^1.0.5",
        "@types/passport-local": "^1.0.33",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "cross-env": "^7.0.3",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "module-alias": "^2.2.2",
        "prettier-eslint": "^12.0.0",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.1.3"
    },
    "_moduleAliases": {
        "@src": "./dist",
        "@middlewares": "./dist/middlewares"
    }
}
